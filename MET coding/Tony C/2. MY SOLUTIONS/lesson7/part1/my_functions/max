#include<stdio.h>
#include<string.h>
#include<stdlib.h>

// A function that will return the maximum of an array
int maximum(int list[]){ //This is a int function because it will return a number which is an int.
    //int size = sizeof(list)/sizeof(list[0]);
    int i,max_no;
    max_no = list[0];
    for(i=1; i<SIZE; i++){
        if (max_no < list[i])
            max_no = list[i];
    }
    return max_no;
}