#include<stdio.h>
#include<string.h>
#include<stdlib.h>

// A function that will return the mean of an array
float mean_fx(float list[]){ //This is a int function because it will return a number which is an int.

    int i,sum;
    sum = 0;
    for(i=0; i<SIZE; i++){
        sum += list[i];
    }
    float mean = sum/(float)SIZE;
    return mean;
}