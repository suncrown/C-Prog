#include<stdio.h>
#include<string.h>
#include<stdlib.h>

// A function that will return the minimum of an array
int minimum(int list[]){ //This is a int function because it will return a number which is an int.
    //int size = sizeof(list)/sizeof(list[0]);
    int i,min_no;
    min_no = list[0];
    for(i=1; i<SIZE; i++){
        if (min_no > list[i])
            min_no = list[i];
    }
    return min_no;
}