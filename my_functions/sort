#include<stdio.h>

// the value for size is in the main prog
void sort(int hold[size]){
    int i,j,temp;
    for(i=0;i<size;i++){
        for(j=i+1;j<size;j++){
            if (hold[i] > hold[j]){
                temp = hold[i];
                hold[i]=hold[j];
                hold[j]=temp;
            }

        }

    }

    for (i=0;i<size;i++){
        printf("%d",hold[i]);
    }
    printf("\n");
}